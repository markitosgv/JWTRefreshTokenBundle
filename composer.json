{
  "name" : "gesdinet/jwt-refresh-token-bundle",
  "description" : "Implements a refresh token system over Json Web Tokens in Symfony",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Marcos GÃ³mez Vilches",
    "email" : "marcos@gesdinet.com"
  }],
  "keywords" : [
    "jwt refresh token bundle symfony json web"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=5.3.3",
    "symfony/framework-bundle": "~2.3|~3.0",
    "symfony/validator": "~2.3|~3.0",
    "doctrine/orm": "^2.4.8",
    "doctrine/doctrine-bundle": "~1.4",
    "lexik/jwt-authentication-bundle": "^1.1|^2.0@dev"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0",
    "henrikbjorn/phpspec-code-coverage": "~1.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\spec\\": "spec/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0-dev"
    }
  }
}
